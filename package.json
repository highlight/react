{
  "name": "@highlight-run/react",
  "version": "1.1.12",
  "description": "The official Highlight SDK for React",
  "main": "dist/index.js",
  "license": "MIT",
  "dependencies": {
    "babel-plugin-css-modules-transform": "^1.6.2",
    "typescript": "^4.1.2"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile": "npm run clean && cross-env NODE_ENV=production babel src/components --extensions \".ts,.tsx\" --out-dir dist --copy-files",
    "build:package": "npm run clean && npm run build:types && npm run build:js",
    "build:js": "babel src/components --out-dir dist --extensions \".ts,.tsx\" --source-maps inline --copy-files",
    "build:types": "tsc --emitDeclarationOnly"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.4",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "cross-env": "^7.0.3",
    "rimraf": "^3.0.2",
    "typescript-plugin-css-modules": "^3.4.0"
  },
  "files": [
    "/dist",
    "package.json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/highlight-run/react"
  },
  "resolutions": {
    "postcss": "7.0.36"
  }
}
